          <zeebe:input source="=if round half up(random number(), 0) = 1 then &#34;https://dummyjson.com/http/404/The selected animal is not available for adoption&#34; else &#34;https://dummyjson.com/http/200/success&#34;" target="url" />
          <zeebe:input source="=false" target="storeResponse" />
          <zeebe:input source="=20" target="connectionTimeoutInSeconds" />
          <zeebe:input source="=20" target="readTimeoutInSeconds" />
          <zeebe:input source="=false" target="ignoreNullValues" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
