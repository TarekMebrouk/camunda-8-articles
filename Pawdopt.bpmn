          <zeebe:input source="=20" target="readTimeoutInSeconds" />
          <zeebe:input source="=true" target="ignoreNullValues" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultExpression" value="={ animals : response.body.animals }" />
